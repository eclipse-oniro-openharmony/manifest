<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote fetch="../openharmony" name="origin" review="https://openharmony.gitee.com/openharmony/"/>
  <remote fetch="https://gitee.com/openharmony-sig" name="sig" review="https://gitee.com/openharmony-sig/"/>
  <default remote="origin" revision="master" sync-j="4" />

  <include name="default.xml"/>
  <project name="device_soc_telink" path="device/soc/telink" groups="default,ohos:mini,ohos:small" revision="master" remote="sig"/>
  <project name="device_board_telink" path="device/board/telink" groups="default,ohos:mini,ohos:small" revision="master" remote="sig"/>
  <project name="vendor_telink" path="vendor/telink" groups="default,ohos:mini" revision="master" remote="sig"/>
</manifest>